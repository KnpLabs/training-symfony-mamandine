{% extends 'layout.html.twig' %}

{% block content %}
<section class="container">
    <h1 class="text-center">Importation des données</h1>
    <button class="btn btn-success mt-3 my-2 my-sm-0" onclick="importUsersAjax()">Importer les données</button>
    <h2 class="mt-3">Importation des utilisateurs</h2>
    <div class="progress mt-3">
        <div class="progress-bar progress-bar-striped" id="progressbar-import" role="progressbar" style="width: 0%" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
    </div>
</section>
{% endblock %}

{% block javascripts %}
  {{ parent() }}
  {{ encore_entry_script_tags('import') }}
{% endblock %}
